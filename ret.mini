fun foo(int i) int      # standard case
{
   return 2; #original: return true;
}

fun bar(int i) int      # control flow case
{
   if (i > 0)
   {
      return i;
   }
   else
   {
      if (i < 0)
      {
         return -1;
      }
      else
      {
         return 0;
      }
   }
}

fun main() void
{
   print bar(2) endl;
}
